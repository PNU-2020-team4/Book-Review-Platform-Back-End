<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.team4.bookreview.reviewMapper">
 
    <select id="selectAll" resultType="com.team4.bookreview.vo.ReviewVO">
        SELECT *
        FROM review
    </select>
    
    <select id="select" resultType="com.team4.bookreview.vo.ReviewVO">
    	SELECT * 
    	FROM review
    	WHERE idx = ${idx}
    </select>
    
</mapper>
